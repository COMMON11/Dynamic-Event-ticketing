<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>RegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserUpdateServlet</servlet-name>
        <servlet-class>UserUpdateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteUserSevlet</servlet-name>
        <servlet-class>DeleteUserSevlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateEventsServlet</servlet-name>
        <servlet-class>CreateEventsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllEventsServlet</servlet-name>
        <servlet-class>GetAllEventsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetEventByIdServlet</servlet-name>
        <servlet-class>GetEventByIdServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetUserDetailsSevlet</servlet-name>
        <servlet-class>GetUserDetailsSevlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookingServlet</servlet-name>
        <servlet-class>BookingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckUserBookingServlet</servlet-name>
        <servlet-class>CheckUserBookingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetUserEventsServlet</servlet-name>
        <servlet-class>GetUserEventsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetUserBookingsServlet</servlet-name>
        <servlet-class>GetUserBookingsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteEventServlet</servlet-name>
        <servlet-class>DeleteEventServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetUserArchivedServlet</servlet-name>
        <servlet-class>GetUserArchivedServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EventUpdateServlet</servlet-name>
        <servlet-class>EventUpdateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserUpdateServlet</servlet-name>
        <url-pattern>/userUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteUserSevlet</servlet-name>
        <url-pattern>/deleteUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateEventsServlet</servlet-name>
        <url-pattern>/createEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllEventsServlet</servlet-name>
        <url-pattern>/getAllEvents</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetEventByIdServlet</servlet-name>
        <url-pattern>/getEventById</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUserDetailsSevlet</servlet-name>
        <url-pattern>/getUserDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookingServlet</servlet-name>
        <url-pattern>/book</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckUserBookingServlet</servlet-name>
        <url-pattern>/checkUserBooking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUserEventsServlet</servlet-name>
        <url-pattern>/getUserEvents</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUserBookingsServlet</servlet-name>
        <url-pattern>/getUserBookings</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteEventServlet</servlet-name>
        <url-pattern>/deleteEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUserArchivedServlet</servlet-name>
        <url-pattern>/getUserArchived</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EventUpdateServlet</servlet-name>
        <url-pattern>/eventUpdate</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
